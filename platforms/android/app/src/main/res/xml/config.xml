<?xml version='1.0' encoding='utf-8'?>
<widget id="mnactivist.app" version="2.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>MnActivist</name>
    <description>
        An app connecting constituents to calendars, and more.
    </description>
    <author email="info@screamfreely.org" href="http://screamfreely.org">
        ScreamFreely Team
    </author>
    <content src="dist/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="res/icon/android/icon.mn.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
